<?xml version="1.0"?>
<Devices>
    <Device id="camect" type="custom">
        <Name>Camect NVR</Name>
        <ConfigUI>
            <Field id="address" type="textfield"  defaultValue="" tooltip="Enter IP address or Hostname of device.">
				<Label>IP Address or Hostname:</Label>
			</Field>
			<Field id="port" type="textfield" defaultValue="443" tooltip="Port number">
				<Label>Port:</Label>
			</Field>
			<Field id="username" type="textfield" defaultValue="admin" tooltip="User Name">
				<Label>User Name:</Label>
			</Field>
			<Field id="password" type="textfield" defaultValue="" tooltip="Password">
				<Label>Password:</Label>
			</Field>
        </ConfigUI>
        <States>
            <State id="status" readonly="true">
                <ValueType >String</ValueType>
                <TriggerLabel>Status</TriggerLabel>
                <ControlPageLabel>Status</ControlPageLabel>
            </State>
            <State id="last_event">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event</TriggerLabel>
                <ControlPageLabel>Last Event</ControlPageLabel>
            </State>
            <State id="last_event_time">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event Time</TriggerLabel>
                <ControlPageLabel>Last Event Time</ControlPageLabel>
            </State>
            <State id="last_event_type">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event Type</TriggerLabel>
                <ControlPageLabel>Last Event Type</ControlPageLabel>
            </State>
            <State id="last_event_desc">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event Description</TriggerLabel>
                <ControlPageLabel>Last Event Description</ControlPageLabel>
            </State>
            <State id="last_event_url">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event URL</TriggerLabel>
                <ControlPageLabel>Last Event URL</ControlPageLabel>
            </State>
            <State id="last_event_cam_id">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event Camera ID</TriggerLabel>
                <ControlPageLabel>Last Event Camera ID</ControlPageLabel>
            </State>
            <State id="last_event_cam_name">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event Camera</TriggerLabel>
                <ControlPageLabel>Last Event Camera</ControlPageLabel>
            </State>
            <State id="last_event_detected">
                <ValueType >String</ValueType>
                <TriggerLabel>Last Event Objects</TriggerLabel>
                <ControlPageLabel>Last Event Objects</ControlPageLabel>
            </State>
           <State id="name">
                <ValueType >String</ValueType>
                <TriggerLabel>Name</TriggerLabel>
                <ControlPageLabel>Name</ControlPageLabel>
            </State>
            <State id="cloud_url">
                <ValueType >String</ValueType>
                <TriggerLabel>Cloud URL</TriggerLabel>
                <ControlPageLabel>Cloud URL</ControlPageLabel>
            </State>
            <State id="local_https_url">
                <ValueType >String</ValueType>
                <TriggerLabel>Local URL</TriggerLabel>
                <ControlPageLabel>Local URL</ControlPageLabel>
            </State>
            <State id="mode">
                <ValueType >String</ValueType>
                <TriggerLabel>mode</TriggerLabel>
                <ControlPageLabel>Mode</ControlPageLabel>
            </State>
            <State id="id">
                <ValueType >String</ValueType>
                <TriggerLabel>ID</TriggerLabel>
                <ControlPageLabel>ID</ControlPageLabel>
            </State>
        </States>
        <UiDisplayStateId>status</UiDisplayStateId>
    </Device>
</Devices>
 
 
 